   public static String vd7( String name){
        return  ( "Hello" + name);}
    public static String vd8(String str){
        String reverseString="";
        int length = str.length();
        for ( int i = length - 1 ; i >= 0 ; i-- )
            reverseString = reverseString + str.charAt(i);
        if (str.equals(reverseString))
            System.out.println(true);
        else
            System.out.println(false);
            return reverseString;}
            public static int vd9(int i, int j) {
            if (j == 0) return i;
            else
            return vd9(j, i % j);}
        public static boolean vd10 (String text) {
            text =(text.toLowerCase());
            if (text == null || text.trim().isEmpty()) {
            return false;}
            return  text.contains(  "a"+"b"+"c"+"d"+"e"+"f"+"g"+"h"+
                                    "i"+"j"+"k"+"l"+"m"+"n"+"o"+"p"+
                                    "q"+"r"+"s"+"t"+"u"+"v"+"w"+"x"+
                                    "y"+"z");}
             public static boolean vd11(int n){
        if (n == 2 || n == 3) System.out.println("2 la` so nguyen to");
        else {
            if (n == 2 || n == 3) System.out.println("2 la` so nguyen to");
            else {
           if (n>2 && (n%3!=0 && n%2!=0))
           {System.out.println(n +" la so nguyen to"); return true;}
           else System.out.println(n + " khong phai la so nguyen to"); return false;}}
            return vd11(n); }   

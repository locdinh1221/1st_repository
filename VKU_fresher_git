    public static String vd14 (float n) {
        int k[] = new int[20];
        int index = 0;
        while (n>=1) {
         k[index++] = (int) (n%2);
          n = n/2;}  
         for (int i = index-1; i >=0; i--)
        System.out.print(k[i]);
        return "";}  

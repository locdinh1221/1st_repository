         public class vd18 {
         public static int vd18 (long n){
         long totalnumber = 1; int out = 0;
          for (int i = 2; i <= n; i++)
         totalnumber = totalnumber *i;
          do { totalnumber = totalnumber/10;
             out = out +1;
          } while ( totalnumber % 10 == 0);
           return out; }}
                
        
